{
  "name": "athena",
  "version": "1.0.0",
  "description": "Full-stack application with TypeScript and Supabase",
  "main": "index.js",
  "scripts": {
    "install:api": "cd api-src && npm install",
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:server\"",
    "dev:api": "cd api-src && npm run dev",
    "dev:server": "node server.js",
    "build": "cd api-src && npm run build",
    "build:vercel": "npm run install:api && npm run build",
    "start": "concurrently \"npm run start:api\" \"npm run start:server\"",
    "start:api": "cd api-src && npm start",
    "start:server": "node server.js"
  },
  "keywords": [
    "typescript",
    "supabase",
    "nodejs"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "@vercel/node": "^3.2.29",
    "http-proxy-middleware": "^3.0.5"
  }
}
